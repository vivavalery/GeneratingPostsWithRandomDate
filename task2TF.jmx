              <stringProp name="script"> import groovy.time.TimeCategory
 
 int counter = Integer.parseInt(vars.get(&quot;counter&quot;)) + 1;
 vars.put(&quot;counter&quot;, Integer.toString(counter)); 
 log.info(&quot;counter&quot; + counter)
 
 if (counter &lt;= ${countForLoop} * 0.9) {
	def start = Date.parse(&apos;yyyy-MM-dd hh:mm&apos;, &apos;2017-01-01 00:00&apos;);
	def end   = Date.parse(&apos;yyyy-MM-dd hh:mm&apos;, &apos;2017-12-31 23:59&apos;);
	random = new Random(); 
	date = randomDate(start..end).format(&quot;yyyy-MM-dd hh:mm&quot;);
	} else {
	date = vars.get(&quot;dates&quot;).toString();
	}
 vars.put(&quot;date&quot;, date);
 log.info(date)

 Date randomDate(Range&lt;Date&gt; range) {
   def res = range.from + random.nextInt(range.to - range.from + 1);
   use(TimeCategory) {
   	res = res + random.nextInt(24).hours + random.nextInt(60).minutes;
   	}
   	res
   	}
